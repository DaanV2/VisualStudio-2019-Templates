<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>$projectname$</id>
        <version>1.0.0</version>
        <description></description>
        <authors>$username$</authors>
		<title>$projectname$</title>

        <!-- Optional elements -->
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="file">LICENSE.txt</license>
        <projectUrl></projectUrl>
        <copyright>$username$ $year$</copyright>
        <tags></tags>
        <repository type="git" url="" />
        <releaseNotes></releaseNotes>
        <icon>images\icon.png</icon>
        <dependencies>
            <group targetFramework=".NETCoreApp3.0" />
            <group targetFramework=".NETCoreApp3.1" />
            <group targetFramework=".NETFramework4.8" />
            <group targetFramework=".NETStandard2.0" />
            <group targetFramework=".NETStandard2.1" />
        </dependencies>

        <!-- ... -->
    </metadata>
    <!-- Optional 'files' node -->
    <files>
        <file src="..\Nuget\images\icon.png" target="images\" />
        <file src="..\Source\LICENSE.txt" target="" />

        <!-- Any CPU -->
        <file src="lib\net48\*.dll" target="lib\net48\" />
        <file src="lib\net48\*.xml" target="lib\net48\" />
        <file src="lib\netcoreapp3.0\*.dll" target="lib\netcoreapp3.0\" />
        <file src="lib\netcoreapp3.0\*.xml" target="lib\netcoreapp3.0\" />
        <file src="lib\netcoreapp3.1\*.dll" target="lib\netcoreapp3.1\" />
        <file src="lib\netcoreapp3.1\*.xml" target="lib\netcoreapp3.1\" />
        <file src="lib\netstandard2.0\*.dll" target="lib\netstandard2.0\" />
        <file src="lib\netstandard2.0\*.xml" target="lib\netstandard2.0\" />
        <file src="lib\netstandard2.1\*.dll" target="lib\netstandard2.1\" />
        <file src="lib\netstandard2.1\*.xml" target="lib\netstandard2.1\" />
				
		<!-- x64 -->
		<!-- Future stuff 
		<file src="runtimes\win-x64\lib\net48\*.dll" target="runtimes\win10-x64\lib\net48\" />
		<file src="runtimes\win-x64\lib\netcoreapp3.0\*.dll" target="runtimes\win10-x64\lib\netcoreapp3.0\" />
        <file src="runtimes\win-x64\lib\netcoreapp3.1\*.dll" target="runtimes\win10-x64\lib\netcoreapp3.1\" />
        <file src="runtimes\win-x64\lib\netstandard2.0\*.dll" target="runtimes\win10-x64\lib\netstandard2.0\" />
        <file src="runtimes\win-x64\lib\netstandard2.1\*.dll" target="runtimes\win10-x64\lib\netstandard2.1\" />
		-->
		
		<!-- x86 -->
		<!-- Future stuff 
		<file src="runtimes\win-x86\lib\net48\*.dll" target="runtimes\win10-x86\lib\net48\" />
		<file src="runtimes\win-x86\lib\netcoreapp3.0\*.dll" target="runtimes\win10-x86\lib\netcoreapp3.0\" />
        <file src="runtimes\win-x86\lib\netcoreapp3.1\*.dll" target="runtimes\win10-x86\lib\netcoreapp3.1\" />
        <file src="runtimes\win-x86\lib\netstandard2.0\*.dll" target="runtimes\win10-x86\lib\netstandard2.0\" />
        <file src="runtimes\win-x86\lib\netstandard2.1\*.dll" target="runtimes\win10-x86\lib\netstandard2.1\" />
		-->		
    </files>
</package>